{
  "dashboard": {
    "id": null,
    "title": "Notrus Application Monitoring",
    "tags": ["notrus"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API HTTP Requests Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(notrus_api_http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Response Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(notrus_api_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Web App Page Views",
        "type": "stat",
        "targets": [
          {
            "expr": "notrus_web_page_views_total",
            "legendFormat": "Page Views"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "notrus_web_active_users",
            "legendFormat": "Active Users"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 5,
        "title": "Agent Interactions",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(notrus_api_agent_interactions_total[5m])",
            "legendFormat": "{{type}}"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-30m",
      "to": "now"
    },
    "refresh": "5s"
  }
}
